<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <NVIDASDK_HOME>$(SolutionDir)..\..\nvidia_video_sdk\nvidia_video_sdk_6.0.1\Samples\common</NVIDASDK_HOME>
    <NVIDASDK_INCLUDE>$(NVIDASDK_HOME)\inc</NVIDASDK_INCLUDE>
    <NIVIDSDK_LIB>$(NVIDASDK_HOME)\lib</NIVIDSDK_LIB>
  </PropertyGroup>
  <PropertyGroup>
    <IncludePath>$(NVIDASDK_INCLUDE);$(IncludePath)</IncludePath>
    <LibraryPath>$(NVIDASDK_LIB);$(LibraryPath)</LibraryPath>
  </PropertyGroup>
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="NVIDASDK_HOME">
      <Value>$(NVIDASDK_HOME)</Value>
    </BuildMacro>
    <BuildMacro Include="NVIDASDK_INCLUDE">
      <Value>$(NVIDASDK_INCLUDE)</Value>
    </BuildMacro>
    <BuildMacro Include="NIVIDSDK_LIB">
      <Value>$(NIVIDSDK_LIB)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>